{
  "name": "alexkart/typed-registry-laravel",
  "description": "Laravel integration for typed-registry with type-casting providers and facades",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "typed-registry",
    "config",
    "env",
    "phpstan",
    "strict",
    "static-analysis"
  ],
  "authors": [
    {
      "name": "TypedRegistry Contributors"
    }
  ],
  "require": {
    "php": "^8.3",
    "alexkart/typed-registry": "^0.1",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/contracts": "^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^11.0",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "TypedRegistry\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TypedRegistry\\Laravel\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "TypedRegistry\\Laravel\\TypedRegistryServiceProvider"
      ],
      "aliases": {
        "TypedEnv": "TypedRegistry\\Laravel\\Facades\\TypedEnv",
        "TypedConfig": "TypedRegistry\\Laravel\\Facades\\TypedConfig"
      }
    }
  },
  "scripts": {
    "test": "phpunit",
    "phpstan": "phpstan analyse"
  }
}
